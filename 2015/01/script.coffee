instructions = instructions.split('')
floor = 0
guide =
	up: '('
	down: ')'
basementReachedOn = undefined
	
for instruction, index in instructions
	if instruction is guide.up
		floor++
	else 	if instruction is guide.down
		floor--
		
	if not basementReachedOn and floor is -1
		basementReachedOn = index+1
		
document.getElementById("part1").innerHTML = "Santa is on floor #{floor}"
document.getElementById("part2").innerHTML =  "Reached -1 for the first time on #{basementReachedOn}"
